# Editor configuration, see http://editorconfig.org
root = true

[*]
charset = utf-8
indent_style = space
end_of_line = lf
insert_final_newline = true

[*.java]
indent_size = 4

[*.{ts, js, json}]
trim_trailing_whitespace = true
indent_size = 2

[*.md]
max_line_length = off
trim_trailing_whitespace = false

